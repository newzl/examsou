<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta name="renderer" content="webkit" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title - 新智联后台</title>
    <link href="/layui/css/layui.css?v=2.2.4" rel="stylesheet" />
    <link href="/areas/css/global.css?v=2.0.2" rel="stylesheet" />
    @RenderSection("css", false)
</head>
<body>
    <div class="layui-layout layui-layout-admin">
        <div class="layui-header layui-bg-exam">
            <div class="layui-logo">
                <div class="topbar"><i class="layui-icon">&#xe60f;</i></div>
                <span>新智联后台</span>
            </div>
            <ul class="layui-nav layui-layout-right">
                <li class="layui-nav-item">
                    <a href="javascript:;">个人中心</a>
                    <dl class="layui-nav-child">
                        <dd><a href="/admin">后台首页</a></dd>
                        <dd><a href="/">网站首页</a></dd>
                        <dd><a href="/">基本资料</a></dd>
                        <dd><a href="/admin/account/quitlogin">安全退出</a></dd>
                    </dl>
                </li>
            </ul>
        </div>
        <div class="layui-side layui-bg-black" id="my_side">
            <div class="my-topnav">全部导航</div>
        </div>
        <div class="layui-body">
            @RenderBody()
        </div>
    </div>
    <script src="//cdn.bootcss.com/jquery/3.2.0/jquery.min.js"></script>
    <script src="/layui/layui.js?v=2.2.4"></script>
    <script src="/areas/admin/js/global.js?v=2.0.2"></script>
    @RenderSection("js", false)
</body>
</html>