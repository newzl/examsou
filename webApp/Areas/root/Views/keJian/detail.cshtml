@model Models.keJian

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>课件查看</title>
</head>
<body>
    <div style="position: relative;height: 120px;padding: 5px">
        <div>
            <span style="font-size: 20px;color: #555;line-height: 30px;font-weight: bold;">@Model.title</span>
            <span style="margin-left: 10px;color: #777;">@Model.author</span>
        </div>
        <div style="color: #aaa;height: 88px;font-size: 14px;cursor: default;">
            @*@Model.cont*@   @(new HtmlString(Model.cont))
        </div>
    </div>
</body>
</html>
